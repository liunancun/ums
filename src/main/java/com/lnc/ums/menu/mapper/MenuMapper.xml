<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lnc.ums.menu.mapper.MenuMapper">

	<select id="query" parameterType="MenuPo" resultType="MenuPo">
		SELECT *
		FROM T_MENU
		<where>
            <if test="admin !=null">
                AND ADMIN = #{admin}
            </if>
        </where>
	</select>

</mapper>